
//using set 
class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        set<int>s;
        for(int i=0;i<nums.size();i++)
        {
            s.insert(nums[i]);
        }
        int i=0;
        for(auto &x:s){
            nums[i]=x;
            i++;
        }
        return s.size();
    }
};


